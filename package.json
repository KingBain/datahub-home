{
  "name": "fsdh",
  "description": "Documentation for Federal Science Data Hub",
  "version": "0.1.0",
  "license": "MIT",
  "author": "fsdh",
  "scripts": {
    "dev": "hugo server --environment development --watch",
    "build": "hugo --gc --minify --templateMetrics --templateMetricsHints --forceSyncStatic",
    "preview": "hugo server --disableFastRender --navigateToChanged --templateMetrics --templateMetricsHints --watch --forceSyncStatic -e production --minify",
    "lint": "markdownlint-cli2 'content/**/*.md' --ignore node_modules",
    "format": "prettier -w ."
  },
  "devDependencies": {
    "prettier": "3.5.0",
    "prettier-plugin-go-template": "0.0.15"
  }
}
